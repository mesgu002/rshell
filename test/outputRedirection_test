echo "Starting Output Redirection Tests..."

ls > OutputTest1
ps > OutputTest2
echo this is a test > OutputTest3
echo this sentence has five words | wc > OutputTest4
echo hello world > OutputTest1
cat < outputRedirection_test > OutputTest2
echo test > OutputTest5
ls > OutputTest1
rm OutputTest1
rm OutputTest2
rm OutputTest3
rm OutputTest4
rm OutputTest5

echo exiting...
exit
